<%= label_tag :created_by %>
<%= content_tag :p, "#{event.host.name}" %>


<%= label_tag :date %>
<%= content_tag :p, "#{event.date}" %>

<%= label_tag :description %>
<%= content_tag :p, "#{event.description}" %>

<%= label_tag :attending %>
<ul>
<% event.attendees.each do |attendee| %>
  <%= content_tag :li, attendee.name %>
<% end %>
</ul>